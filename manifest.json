{
	"name": "ChromeIDE",
	"description": "ChromeIDE",
	"manifest_version": 2,
	"version": "0.2",
	"app": {
		"background": {
			"scripts": ["src/background.js"]
		}
	},
	"permissions": ["webview", "tabs"],
	"icons": {
		"128": "icon.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}